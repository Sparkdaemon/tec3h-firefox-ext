{
  "manifest_version": 2,
  "name": "Tec3h",
  "description": "Utilitaire Tec3h",
  "version": "1.0",
  "browser_action": {
    "default_icon": "img/favicon.png",
   "default_popup": "popup.html"
  },
  "content_scripts": {
      "matches" :
                ["http://*/*",
                "https://*/*",
                "tabs",
                "storage",
                "background",
                "contextMenus",
                "notifications",
                "https://fr-opv.peugeot.com/",
                "https://fr-opv.peugeot.com/*"]
            "js": 
                ["js/app.js", "js/jquery-3.4.1.min.js"]
        },
  "icons": {
          "16": "img/favicon.png",
          "48": "img/favicon.png",
          "128": "img/favicon.png"
        }
}
